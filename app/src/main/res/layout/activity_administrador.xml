<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layoutadmin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".administrador">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layadmin"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/borrarLink_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ADMINISTRAR"
            app:layout_constraintBottom_toTopOf="@+id/guideline39"
            app:layout_constraintEnd_toStartOf="@+id/guideline15"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toTopOf="@+id/guideline38" />

        <Button
            android:id="@+id/administrar_libro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="administrar"
            app:layout_constraintBottom_toTopOf="@+id/guideline14"
            app:layout_constraintEnd_toStartOf="@+id/guideline15"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toTopOf="@+id/guideline13" />

        <TextView
            android:id="@+id/editorial_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="EDITORIAL"
