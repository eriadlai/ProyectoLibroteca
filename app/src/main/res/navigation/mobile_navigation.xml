<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.libroapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_busqueda"
        android:name="com.example.libroapp.ui.Busqueda"
        android:label="@string/title_busqueda"
        tools:layout="@layout/fragment_busqueda" />
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.libroapp.ui.dashboard.DashboardFragment"
        android:label="@string/title_colecciones"
        tools:layout="@layout/fragment_colecciones" />
    <fragment
        android:id="@+id/favoritos"
        android:name="com.example.libroapp.Favoritos"
        android:label="fragment_favoritos"
        tools:layout="@layout/fragment_favoritos" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.libroapp.ui.notifications.NotificationsFragment"
        android:label="@string/title_historial"
        tools:layout="@layout/fragment_notifications" />


</navigation>
